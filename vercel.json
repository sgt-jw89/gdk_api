{
	"version": 2,
	"public": true,
	"regions": ["fra1"],
	"rewrites": [{ "source": "/(.*)", "destination": "/api/$1" }],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
		},
		{
			"source": "https://gdk-api-2ns2.vercel.app/(.*)",
			"headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
		}
	]
}
